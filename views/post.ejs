<!DOCTYPE HTML>
<html>
<head>
    <%- include ./partials/head.ejs %>
</head>
<body>
	<%- include ./partials/banner.ejs %>
   <div class="main">
    
    <div class="middle_content">
    	<div class="container">
            <h2>Welcome to GTimes</h2>
            <% data.forEach(element=>{ %>
                <p> username = <%= element.username %></p>
                <p> reputasi = <%= element.reputasi %></p>
                <% element.Posts.forEach(singlePost=>{ %>
                    <p> Post = <%= singlePost.title %> </p>
                        <% var countLike = 0 %>
                        <% var countDislike = 0 %>
                    <% singlePost.PostLikes.forEach(dataLike=>{ %>
                        
                        <% if(dataLike.Like == true) { %>
                            <% countLike++ %>
                       <% } else if(dataLike.Like == false) { %>
                        <% countDislike++ %>
                      <% } %>
                        
                 <% }) %>
                 <p> Like = <%= countLike %></p>
                        <p> Dislike = <%= countDislike %></p>
                 <% }) %>                    
               <% }) %>                
            
    		
    	</div>
    </div>
   	
   </div>
   <%- include ./partials/footer.ejs %>
</body>
</html>		